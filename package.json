{
  "name": "workspace",
  "private": true,
  "version": "1.0.0",
  "description": "Workspace for multiple applications and shared packages.",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.12.0"
  },
  "scripts": {
    "test": "pnpm run packages:test",
    "mobile": "pnpm --filter 'mobile' start",
    "web": "pnpm --filter 'web' dev",
    "packages:build": "pnpm --filter './packages/**' build",
    "packages:watch": "pnpm --filter './packages/**' watch",
    "packages:test": "pnpm --filter './packages/**' test",
    "db:migrate": "pnpm --filter '@-/db' migrate",
    "db:studio": "pnpm --filter '@-/db' studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "overrides": {
    "semver": "7.5.2"
  },
  "devDependencies": {
    "@-/theme": "workspace:*",
    "@apollo/client": "^3.10.4",
    "@apollo/server": "^4.10.4",
    "@babel/core": "^7.24.5",
    "@chromatic-com/storybook": "^1.4.0",
    "@do-ob/eslint-config": "^1.2.0",
    "@do-ob/ts-config": "^1.1.0",
    "@do-ob/vite-lib-config": "^2.1.3",
    "@expo/metro-runtime": "^3.2.1",
    "@expo/webpack-config": "^19.0.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@parcel/watcher": "^2.4.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/nextjs": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.2",
    "drizzle-orm": "^0.30.10",
    "eslint": "^9.2.0",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "expo": "^51.0.5",
    "expo-status-bar": "^1.12.1",
    "expo-system-ui": "^3.0.4",
    "graphql": "^16.8.1",
    "nativewind": "^4.0.36",
    "next": "^14.2.3",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postgres": "^3.4.4",
    "react": "19.0.0-rc-915b914b3a-20240515",
